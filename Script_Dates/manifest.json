{
    "description": "Echange url <-> date",
    "manifest_version": 2,
    "name": "date_getter_ext",
    "version": "1.0",
    "icons": {
	"48": "icons/message.svg"
    },

    "applications": {
	"gecko": {
	    "id": "date_getter@ptut",
	    "strict_min_version": "50.0"
	}
    },

    "background": {
	"scripts": ["url_sender.js"]
    },

    "browser_action": {
	"default_icon": "icons/message.svg",
	"browser_style": true
    },

    "permissions": ["nativeMessaging"]
}
