{

  "manifest_version": 2,
  "name": "OCSP Checker Notif",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["notif.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "OCSP Checker Notif",
    "default_popup": "popup.html"
  }

}
