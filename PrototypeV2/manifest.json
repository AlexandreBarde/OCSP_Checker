{
  "description": "Prototype d'OCSP Checker",
  "manifest_version": 2,
  "name": "OCSP Checker",
  "version": "2.0",
  "background": {
    "scripts": [
      "src/addon/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "src/addon/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "128": "assets/icons/icon.png"
  },
  "browser_action": {
    "default_icon": "assets/icons/icon.png"
  },
  "permissions": [
    "nativeMessaging",
    "webNavigation",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
